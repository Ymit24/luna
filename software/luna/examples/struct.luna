const puts = fn@extern["puts"](fmt: *i8):i32;
const printf = fn@extern["printf"]@variadic(fmt: *i8): i32;

const pair = struct {
    a: i32,
    b: i32
};

const main = fn(): i32 {
    puts("hello world");
    let p: pair = .pair {
        x: 0,
        y: 1
    };
    printf("pair: %d,%d\n", p.x, p.y);
    return 0;
};
