const printf = fn@extern["printf"]@variadic(fmt: *i8);
const puts = fn@extern["puts"](fmt: *i8);

const main = fn(): i32 {
    const a: i32 = 100;
    if a {
        puts("it worked!");
        let foo = 100;
        let inner = fn() {
            puts("test");
        };
        printf("foo is %d\n", foo);
        if a {
            puts("other");
        };
    };
    return 0;
};
