{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "monkey_base",
      "camelcase": "MonkeyBase",
      "title": "MonkeyBase",
      "scope": "source.monkey_base",
      "file-types": [
        "monkey_base"
      ],
      "injection-regex": "^monkey_base$",
      "class-name": "TreeSitterMonkeyBase"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "MonkeyBase grammar for tree-sitter",
    "authors": [
      {
        "name": "Christian Smith"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-monkey_base"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}