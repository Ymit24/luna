{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "shadow",
      "camelcase": "Shadow",
      "title": "Shadow",
      "scope": "source.shadow",
      "file-types": [
        "shadow"
      ],
      "injection-regex": "^shadow$",
      "class-name": "TreeSitterShadow"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Shadow grammar for tree-sitter",
    "authors": [
      {
        "name": "Christian Smith"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-shadow"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}